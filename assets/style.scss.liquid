
@import url('https://fonts.googleapis.com/css2?family=Oswald:wght@200;300;400;500;600;700&display=swap');
$width: {{ settings.page_width }}px; 
$blacktext: {{ settings.text_color}};
$bp: 750px; 

.h5{
    font-size: 0.8em; 
}

.h4{
    font-size: 0.9em; 
}

.h3{
    font-size: 1.3em; 
}

.h2 {
    font-size: 1.5em; 
}

.h1 {
    font-size: 2em; 
}

.h0 {
    font-size: 3em; 
}

::-webkit-scrollbar {
    width: 10px;
    height: 10px; 
}

::-webkit-scrollbar-track {
    background: #f1f1f1; 
}
 
::-webkit-scrollbar-thumb {
    background: #888; 
}

::-webkit-scrollbar-thumb:hover {
    background: #555; 
}

*{
    margin: 0; 
    padding: 0; 
    box-sizing: border-box; 
}

html, body{
    font-family: 'Oswald', sans-serif;
    height: 100%; 
    width: 100%;
    margin: 0; 
    padding: 0; 
    overflow: none; 
    overflow-x: none; 

    @media only screen and (min-width: 749px){
        font-family: 'Oswald', sans-serif;
        height: 100%; 
        width: 100%;
        margin: 0; 
        padding: 0; 
        overflow: none; 
        overflow-x: hidden; 
    }
}

a{
    text-decoration: none; 
    color: $blacktext; 
}

li{
    list-style: none; 
}

img {
    display: block; 
    width: 100px; 
}

.hero{
    background: #111111; 
}

/*header*/ 

.pc-header{
    width: 100vw;
    padding-bottom: 3vh; 
    background: #ffffff; 
    box-shadow: 0 4px 8px -1px rgba(0,0,0,0.2);

    @media only screen and (max-width: 749px){ 
        display: none; 
    }
}

.header-flex{
    display: flex; 
    align-items: center; 
    justify-content: space-between; 
    max-width: $width; 
    margin: auto; 
}

.menu{
    display: flex; 

}

.icon-item{
    display: flex; 
    flex-direction: row;
}

.menu-item{
    display: block; 
    padding-left: 5vw;
}

.menu-nav p{
    font-weight: 500; 
}

.icon-item svg{
    display: block;
    width: 7vw;
    padding-left: 50px;
    margin-top: 2px;
}

.menu-nav{
    font-size: 19px;
    font-weight: 700; 
}

.mobile-header{
    @media only screen and (min-width: $bp){ 
        display: none; 
    }
}

.mobile-logo {
    display: flex; 
    justify-content: center; 
    position: absolute; 
    top: 10%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 3; 
}

/*hero*/ 

.mobile-banner{
    width: 100vw; 
    height: 100vh;  
    position: relative; 
    overflow: hidden; 

    @media only screen and (min-width: 750px){ 
        display: none; 
    }
}

.mobile-banner .mslide-list{ 
    display: flex; 
    position: absolute; 
    width: 100%; 
    height: 100%; 
    left: 0; 
    top: 0; 
    transition: 1s; 
}

.m-slideshow{
    z-index: 2; 
}

.m-slideshow img{
    width: 100vw;
    height: 100vh;
    object-fit: cover;
    opacity: 0.9; 
}

.desktop-banner{ 
    max-width: 100vw; 
    height: 80vh;  
    position: relative; 
    overflow: hidden; 
    margin-bottom: 3vh; 
    margin-top: -3vh; 

    @media only screen and (max-width: 749px){
        display: none; 
    }
}

.desktop-banner .dslide-list{ 
    display: flex; 
    position: absolute; 
    width: 100%; 
    height: 100%; 
    left: 0; 
    top: 0; 
    transition: 1s;
}

.d-slideshow {
    z-index: 2; 
}

.d-slideshow img{
    width: 100vw;
    height: 80vh;
    object-fit: cover;
    opacity: 0.9; 
}

.desktop-banner .dslide-list .d-slideshow .banner-content{
    position: absolute;
    left: 42%;
    top: 40%;
    width: 80vw;
    max-width: 80%;
    z-index: 3;
}

.d-slideshow.active, 
.banner-content.active {
    opacity: 1;
    z-index: 10;
}

/*featured products*/ 

.desktop-featured{
    margin: 10% 2%; 

    @media only screen and (max-width: 749px){
        margin: 50px 10px;  
    }
}

.desktop-featured img{
    width: 35vw; 
    height: 65vh; 
    padding: 0.1313em 0; 
    object-fit: cover; 

    @media only screen and (max-width: 749px){
        width: 45vw; 
        height: 30vh; 
        object-fit: cover; 
    }
}

.dfeature-row{
    display: flex; 
    flex-direction: row; 
    overflow-x: scroll; 

    @media only screen and (max-width: 749px){
        display:flex; 
        flex-wrap: wrap; 
        justify-content: center; 
    }
}

.dfeature-column{

    @media only screen and (max-width: 749px){
        flex: 0 0 50%; 
        max-width: 50%; 
    }
}

.dfeature-imageslide{
    padding: 0.313em 0.313em; 

    @media only screen and (max-width: 749px){ 
        padding: 0 0.563em; 
    }
}

.dfeature-imageslide h3{
    padding: 5px 0; 

}

/*new-banner*/

.new-box-wrap{ 
    width: 100vw; 
    height: 100vh;  
    position: relative; 
    overflow: hidden; 

    @media only screen and (min-width: 749px){
        display: none; 
    }
}

.slideshow-container {
    position: relative;
    width:  100vw;
    height:  100vh;
}

.slide {
    position: absolute;
    width:  100%;
    height:  100%;
    opacity: 0; 
    transition: opacity  3s;
    display: none; 
}

.slide.active {
    opacity:  1;
    display: block; 
}

.slide:first-child{
    opacity: 1; 
}

.slide img{
    display: block;
    margin: auto; 
    object-fit: contain; 
    margin-top: 5vh; 
    width: 80vw; 
    height: 100%; 
}

.slide h3{
    text-align: center;
    margin-top: 10vh; 
}

.slide p{
    text-align: center; 
    margin: 6vh 4vh; 
}

.image-container{
    position: relative; 
    display: inline; 
}

.upcoming-container{

    @media only screen and (max-width: 749px){
        display: none; 
    }
}

.upcoming_box img{
    width: 100vw;
    height: 80vh;
    object-fit: cover;
    opacity: 0.9; 
}

.upcoming_box button{
    position: absolute; 
    top: 50%; 
    left: 50%; 
    transform: translate(-50%, -50%);
    width: 120px; 
    height: 50px; 
    border: 2px black; 
    box-shadow: 10px 10px 5px 0px rgba(0,0,0,0.75);
    border-radius: 2px; 
    opacity: 0.5; 
}

.upcoming_box button:hover{
    opacity: 1; 
}

/*About Us*/
.aboutus-container{
    position: relative; 
    margin: 10vh 0; 

     @media only screen and (min-width: 749px){
        display: none; 
    }
}

.aboutus-image{
    position: relative; 
}

.aboutus-image img{
    width: 100vw; 
    height: 35vh; 
    z-index: 1
}

.aboutus-content-container{
    position: absolute; 
    top: 50%; 
    left: 50%; 
    transform: translate(-50%, 0%); 
    width: 90vw;
    background-color: #ffffff; 
    box-shadow: 6px 5px 6px 5px rgba(0,0,0,0.36);
}

.aboutus-content-container h3{
    text-align: center; 
}

.aboutus-content{
    margin: 4vh 4vw;  
    line-height: 2.0; 
}

.aboutus-desktop-box{
    margin: 10vh 20vw; 
    width: 60vw; 
    height: 60vh; 

     @media only screen and (max-width: 749px){
        display: none; 
    }
}

.aboutus-boxcontent{
    width: 60vw; 
    height: 60vh; 
    line-height: 2.0; 
    padding: 5vh 5vw; 
}

.aboutus-boxcontent h3{
    text-align: center; 
}

/* newsletter */ 
.newsletter-container {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.newsletter-image {
    position: relative;
    max-width: 100%; 
}

.newsletter-content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    text-align: center;
    z-index: 1; 

    @media only screen and (max-width: 749px){
        display: none; 
    }
}

.newsletter-content-mobile{
    position: absolute;
    top: 70%;
    left: 50%;
    transform: translate(-50%, -50%);
    text-align: center;
    z-index: 1; 
    line-height: 2.0; 

    @media only screen and (min-width: 749px){
        display: none;
    }
}

.newsletter-image img{
    width: 100vw; 
    height: 80vh;   
    object-fit: cover; 
    opacity: 0.9; 

    @media only screen and (max-width: 749px){
        width: 100vw; 
        height: 40vh; 
        margin-top: 30vh; 
    }
}


.newsletter-content h3{
    text-align: center;
}

.newsletter-container button{
    display: none; 
}

.newsletter-content input{
    border: 2px solid black; 
    width: 30vw; 
    height: 6vh; 
    background-color: transparent; 
    margin: 2vh 0; 
    padding: 2vh 1vw; 
    outline: none; 
}

.newsletter-content-mobile input{
    border: 2px solid black; 
    width: 50vw; 
    height: 5vh; 
    background-color: transparent; 
    margin: 2vh 0; 
    padding: 2vh 1vw; 
    outline: none; 
}

/* Footer.liquid */ 
.footer-container{
    width: 100vw; 
    height: 60vh; 
    margin-top: 10vh; 

    @media screen and (max-width: 749px){
        width: 100vw; 
        height: 55vh; 
        margin-top: 10vh; 
    }
}

@media screen and (max-width: 749px) {
    .footer-container.desktop {
        display: none;
    }
    .list-social-item svg{
        transform: translateX(10vw); 
    }

    .list-social-item h4{
        margin-left: 12vw;
    }
}

@media screen and (min-width: 750px) {
 .footer-container.mobile {
    display: none;
 }
}

.footer-headerlogo{
    margin: 0 10vw;
    padding-top: 10vh;
    display: flex; 
    flex-direction: column;
    line-height: 2.5;
}

.footer-wrapper{
    margin: 0 10vw;
    display: flex; 
    flex-direction: row;
    line-height: 2.5;
    
    @media only screen and (max-width: 749px){
        margin: 0 0;
        display: flex; 
        flex-direction: row;
        line-height: 2.5;
    }
}

.footer-container-row{
    padding: 0 10vw 0 0; 

     @media only screen and (max-width: 749px){
        padding: 0 5vw 0 0; 
    }
}

.footer-social{
    padding-top: 2vh;
}

.social-link {
    display: flex;
    align-items: center;
    gap: 2vw;
}

.footer-menu{
    padding-top: 2vh;
}

/*mobile-menu*/
.m-menu{
    position: fixed; 
    bottom: 2vh; 
    right: 2vw; 
    z-index: 1000; 

    @media only screen and (min-width: 750px){
        display: none;
    }
}

.menu-button h4{
    font-weight: bold; 
    font-size: 20px; 
    
}

.menu-button{
    border: none; 
    width: 40vw; 
    height: 8vh; 
    border-radius: 20px; 
    cursor: pointer;  
}

.menu-content{
    display: flex;
    flex-direction: row; 
    wrap: nowrap; 
    align-items:center;
    justify-content:center;
    gap: 2vw;
}

.m-menu svg{
    width:8vw; 
    height:8vh; 
}

.menu-popup{
    position: fixed; 
    top: 50%; 
    left: 50%; 
    transform: translate(-50%, -50%); 
    padding: 20px; 
    border-radius: 20px; 
    box-shadow: 0 0 10px rgba(0,0,0,0.1); 
    width: 80%; 
    max-width: 60vw; 
    z-index: 1001; 
}

.menu-popup .nav{
    list-style-type: none; 
    padding: 0; 
}

.m-menu-item{
    margin-bottom: 1vh; 
}

.menu-nav-content{
    display: flex;
    flex-direction: row; 
    wrap: nowrap; 
    align-items:center;
    justify-content:center;
    gap: 4vw; 
}

.m-menu-nav{
    text-decoration: none; 
}

/* shop.liquid */
.shop_page{
    display: flex; 
    flex-direction: column; 
    justify-content: center; 
    align-items: center;
    gap: 3vh;
    margin-top: 3vh; 

    @media only screen and (max-width: 749px){
        display: none;
    }
}

.shop_banner img{
    width: 100vw; 
    height: 70vh;
    object-fit: cover; 
}

.shop_products{
    margin-top: 5vh; 
    margin-left: 10vw; 
}

.shop_products img{
    width: 35vw; 
    height: 80vh; 
    padding: 2em 0; 
    object-fit: cover; 
}

.shop-row{
    display: flex; 
    flex-wrap: wrap; 
    justify-content: center;
    align-items: center; 
    width: 100%; 
}

.shop-column{
    flex: 0 0 50%; 
    max-width: 50%; 
}

.shop-imageslide{
    padding: 0 0.563em; 
}

.shop-imageslide h3{
    padding: 5px 0; 
}

