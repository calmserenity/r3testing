/*account */ 
.customer_page{
    margin: 2vh 5vw; 
}

.customer_information_container{
    display: flex; 
    flex-direction: row; 
    wrap: nowrap;
    justify-content: space-between;

    @media only screen and (max-width: 749px){
        display: flex; 
        flex-direction: column; 
        justify-content: center;       
        align-items: center; 
    }
}

.customer_information{
    display: flex; 
    flex-direction: column;
    border-radius: 10px;     
    margin: 5vh 0; 
    width: 30vw; 
    height: 35vh;
    
    @media only screen and (max-width: 749px){
        display: flex; 
        justify-content: center; 
        align-items: center;
        border-radius: 10px;     
        margin: 5vh 0; 
        width: 80vw; 
        max-height: 80vh;
    }
}

.customer-info-table{
    border-spacing: 5vw;
    margin-left: 5vw;
    margin-top: 2vh;

    @media only screen and (max-width: 749px){
        border-spacing: 4vw;  
        margin: 1vh 2vw;   
    }
}

.customer_information td{
    @media only screen and (max-width: 749px){
        padding-right: 10px; 
    }
}

.logout-button{
    display: flex; 
    justify-content: center; 
    align-items: center; 
    margin-top:3vh
}

.logout-button button{
    width: 10vw; 
    height: 7vh; 
    border: 1px solid white; 
    border-radius: 30px; 
    background-color: transparent; 

    @media only screen and (max-width: 749px){
        width: 30vw; 
        height: 7vh; 
        border: 1px solid white; 
        border-radius: 30px; 
        background-color: transparent; 
    }
}

.logout-button button:hover{
    background-color: black;
}

.logout-button h5{
    color: white; 
    font-size: 13px; 
    font-family: 'Oswald', sans-serif;
}

.customer_order{
    width: 50vw;
    max-height: 100vh;
    margin-top: 5vh;

    @media only screen and (max-width:749px){
        width: 95vw; 
        max-height: 100vh; 
        margin-top: 10vh;
    }
}


.customer_order td{
    text-align: left;
    width: 3vw;

    @media only screen and (max-width: 749px){
        font-size: 15px;
        width: 10vw;
        padding-bottom: 2vh;
    }
}

.order-items{
    width: 10vw;
}

.customer_order th{
    text-align: left; 
    
    @media only screen and (max-width: 749px){
        text-align: left;
    }
}

.customer_order h2{

    @media only screen and (max-width: 749px){
        text-align: center;
    }
}

/* main-product */ 
.product{
    margin: 15vh 8vw;

    @media only screen and (max-width: 749px){
        margin: 2vh 2vw; 
    }
}

.product-container{
    display: flex; 
    flex-direction: row; 
    flex-wrap: wrap; 
    justify-content: space-between;

    @media only screen and (max-width: 749px){
        display: flex; 
        flex-direction: column; 
        justify-content: space-between; 
    }
}

.product-compare{
    text-decoration: line-through;
}

.hide{
    display: none; 
}

.product-left{
    display: flex; 
    flex-direction: row;
    wrap:nowrap; 
    
    @media only screen and (max-width: 749px){
        display: flex; 
        flex-direction: row; 
        justify-content:center;
    }
}

.product-featured-image{
    width: 40vw;

    @media only screen and (max-width: 749px){
        width: 80vw;
        
    }
}

.product-left img{
    width: 90%;
    object-fit:cover;

    @media only screen and (max-width: 749px){
        width: 100%;
        object-fit:cover;
    }
}

.product-right{
    display: flex; 
    flex-direction: column; 
    width: 35vw; 

    @media only screen and (max-width: 749px){
        display: flex; 
        flex-direction: column; 
        width: 90vw;
        margin-top: 2vh;
        margin-left: 2vh;
    }
}

.product-title{
    font-size: 40px;    
    font-weight: 500;
    margin-bottom: 2vh;

    @media only screen and (max-width: 749px){
        font-size: 40px; 
        font-weight: 500; 
        margin-bottom: 2vh; 
        width: 100%;
    }
}

.product-image-thumbs {
    display: flex; 
    flex-direction: column; 
    wrap: nowrap; 
}

.product-image-thumbs img{
    width: 5vw; 
    height: 15vh; 

    @media only screen and (max-width: 749px){
        width: 10vw; 
        height: 10vh;
    }
}

.selected {
    box-shadow: 20px 20px 50px black;
}

.product-option-quantity{
    margin-top: 0.7rem;
}

.product-option-quantity label{
    font-weight: bold; 
    font-size: 18px;
}

.product-option-quantity svg{
    width: 2vw; 
    height: 2vh;
    background-color: transparent;
}

.quantity-selector {
    display: flex;
    align-items: center;
    border: 1px solid #ccc; 
    border-radius: 2px;  
    width: fit-content;
    margin-top: 1vh;
}

.quantity-selector-minus,
.quantity-selector-plus {
    border: none; 
    background-color: transparent;
    padding: 0.8rem 1rem;
    cursor: pointer; 
}

.quantity-selector-minus{
    border-right: 1px solid #ccc; 
}

.quantity-selector-plus{
    border-left: 1px solid #ccc;
}

.quantity-selector-input {
    text-align: center;
    border: none;
    padding: 8px;
    width: 10vw;
}

.desc-pro{
    display: none;  
}

.desc-dropdown{ 
    cursor: pointer; 
}

.product-description table{
    width: 35vw;    
    height: 10vh;
    margin-top: 3vh;
    border-top: 1px solid black; 
    border-bottom: 1px solid black;

    @media only screen and (max-width: 749px){
        width: 90vw;    
        height: 8vh;
        margin-top: 3vh;
        border-top: 1px solid black; 
        border-bottom: 1px solid black;
    }
}

.desc-title{
    padding-left: 20px;
    vertical-align: middle; 
}

.product-description h3{
    font-weight: 300; 
}

.desc-icon{
    padding-top: 5px;
}

.desc-pro{
    width: 35vw; 
    height: fit-content;
}

.product-description h5{
    padding: 2vh 2vw;
    font-weight: 300; 
    font-size: 16px; 
}

.option-choice{
    display: flex; 
    flex-direction: row; 
    margin: 0.5rem 1rem;
}

.product-option fieldset {
    border: none;
    margin-top: 2vh;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}

.product-option legend {
    font-weight: bold;
    font-size: 18px;
    width: 100%;
    vertical-align: middle; 
}

.product-option div {
  margin-right: 1rem;
  margin-bottom: 1rem;
}

.product-option input[type="radio"] {
    position: absolute;
    opacity: 0;
}

.product-option label {
  display: inline-block;
  padding: 0.5rem 1rem;
  vertical-align: middle; 
  border: 1px solid #ccc;
  border-radius: 3px;
  background-color: #f9f9f9;
  cursor: pointer;
}

.product-option input[type="radio"]:checked + label {
  background-color: #0F0F65;
  color: #ffffff;
}

.product-compare{
    font-size: 20px; 
}

.product-prices{
    padding-top: 5vh;   
}

.product-price{
    font-size: 25px; 
    padding-bottom: 2vh; 
}

.product-add{
    width: 20vw; 
    height: 10vh;
    border-radius: 30px;
    margin-top: 3vh;

    @media only screen and (max-width: 749px){
        width: 35vw; 
        height: 8vh;
        border-radius: 30px;
        margin-top: 3vh;
    }
}

.product-add p{
    font-size: 17px;
    font-family: 'Oswald', sans-serif;
    font-weight: bold;  
}

.abled:hover{
    opacity: 0.7;
}

.add_to_cart_button{
    display: flex; 
    width: auto;
    justify-content: center;
    padding-bottom: 3vh;
}

/* 3d-model */

.model-container{
    display: flex; 
    justify-content: center; 
    margin: 10vh 0;
}


.model-display {
    display: flex; 
    justify-content: center; 
    align-items: center;
    flex-wrap: wrap; 
    background-color: #fefefe;
    width: 60vw; 
    height: 80vh;
    border: 1px solid black; 
    border-radius: 20px; 

    @media only screen and (max-width: 749px){
        display: flex; 
        justify-content: center; 
        align-items: center;
        flex-wrap: wrap; 
        background-color: #fefefe;
        width: 90vw; 
        height: 90vh;
        border: 1px solid black; 
        border-radius: 20px; 

    }
}

.model-container button{
    width: 35vw; 
    height: 8vh;
    color: white; 
    background-color: #0F0F65;
    border-radius: 30px;

    @media only screen and (min-width: 750px){
        display: none;
    }
}

model-viewer{
    width: 70vw; 
    height: 60vh;
}

/* video-gallery */
video{
    width: auto; 
    height: 70vh;
    border-radius: 5px;
    border:none;
    flex: 0 0 auto;
    scroll-snap-align: start;
}

.video-container{
    display: flex;
    flex-wrap: nowrap;
    width: 90vw;
    height: 80vh;
    overflow-x: scroll;
    gap: 1vw;
    
    @media only screen and (max-width: 749px){
        display: flex; 
        flex-direction: row; 
        flex-wrap: wrap; 
        justify-content: center;
        overflow-x:hidden;
    }
}

.video-gallery{
    margin: 2vh 5vw;
}

.video{
    display: flex; 
    align-items: center;
    width: 100vw; 
    height: 80vh;
}

@media only screen and (max-width: 749px){
    .video-gallery h1{
        text-align: center;
    }
}

